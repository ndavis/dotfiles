[core]
  editor = vim
[user]
	name = Nikea Davis
	email = nikeadavis@gmail.com
[credential]
	helper = osxkeychain
[alias]
	co = checkout
	st = status
	nb = checkout -b
  lg = log --graph --abbrev-commit --decorate --date=relative --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
  us = reset HEAD
[color]
    ui = true
[color "status"]
  added = green
  changed = yellow
  untracked = red
[color "diff"]
  meta = blue
  frag = magenta
  old = red
  new = green
  whitespace = white reverse
[color "branch"]
  current = yellow black
  local = yellow
  remote = white
[branch]
	autosetuprebase = always
